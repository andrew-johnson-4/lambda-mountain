
import lib2/core/bedrock.lsts;

type T = { tag:CString };

let .ground-tag-and-arity(t1: T): (CString,U64) = (
   (scope($"if"
      (true)
      ((c"Array", 2_u64))
      (scope($"if"
         (let tag = t1.tag; branchtrue())
         ((tag, 1_u64))
         (fail(c"Pattern Match Failure"))
      ))
   ))
);


