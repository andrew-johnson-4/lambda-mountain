
import lib2/core/bedrock.lsts;

assert( c"a" == c"a" );
assert( c"a" != c"b" );
assert( c"a" <= c"a" );
assert( c"a" < c"b" );
assert( c"a" >= c"a" );
assert( c"b" >= c"a" );
assert( c"b" > c"a" );

assert( cmp(c"a", c"a") == Equal );
assert( cmp(c"a", c"b") == LessThan );
assert( cmp(c"b", c"a") == GreaterThan );

assert( c"".length == 0 );
assert( c"a".length == 1 );
assert( c"ab".length == 2 );

assert( c"abc"[0] == 97 );
assert( c"abc"[1] == 98 );
assert( c"abc"[2] == 99 );

assert( c"a" + c"bc" == c"abc" );
