
import lib2/core/bedrock.lsts;

assert( (None : U64?) == (None : U64?) );
assert( Some(0) == Some(0) );
assert( Some(1) == Some(1) );
assert( Some(0) != Some(1) );
assert( Some(1) != Some(0) );
assert( safe-alloc-block-count == 0 );

assert( (None : String?) == (None : String?) );
assert( Some("0") == Some("0") );
assert( Some("1") == Some("1") );
assert( Some("0") != Some("1") );
assert( Some("1") != Some("0") );
assert( safe-alloc-block-count == 0 );
