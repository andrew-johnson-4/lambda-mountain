
import lib2/core/bedrock.lsts;

assert( 1 == 1 );
assert( 2 == 2 );
assert( not(2 == 1) );
assert( 2 != 1 );
assert( 1 != 2 );
assert( not(1 != 1) );
assert( 2 <= 2 );
assert( 2 <= 3 );
assert( not(2 < 2) );
assert( 2 < 3 );
assert( 2 >= 2 );
assert( 3 >= 2 );
assert( not(2 >= 3) );
assert( 3 > 2 );
assert( not(2 > 2) );

assert( 4 & 4 == 4 );
assert( 12 & 4 == 4 );
assert( 4 | 4 == 4 );
assert( 12 | 4 == 12 );
assert( 4 ^ 4 == 0 );
assert( 12 ^ 4 == 8 );
assert( ~4 == 251 );

assert( 2 + 2 == 4 );
assert( 2 - 2 == 0 );
assert( 5 / 3 == 1 );
assert( 5 % 3 == 2 );
assert( 5 * 3 == 15 );
