
import std/default.lsts;

let li = [1_u64, 2_u64, 2_u64, 3_u64, 4_u64, 5_u64] : List<U64>;
assert( li == [1_u64, 2_u64, 2_u64, 3_u64, 4_u64, 5_u64] );
assert( li.into(type(String)) == "[1,2,2,3,4,5]" );
assert( li.nth(0_u64).get-or(100_u64) == 1 );
assert( li.nth(1_u64).get-or(100_u64) == 2 );
assert( li.nth(2_u64).get-or(100_u64) == 2 );
assert( li.nth(3_u64).get-or(100_u64) == 3 );
assert( li.nth(4_u64).get-or(100_u64) == 4 );
assert( li.nth(5_u64).get-or(100_u64) == 5 );
assert( li.nth(6_u64).get-or(100_u64) == 100_u64 );
assert( li.nth(7_u64).get-or(100_u64) == 100_u64 );
assert( li.nth(200_u64).get-or(100_u64) == 100_u64 );
assert( li.length == 6 );
li = li + ([3_u64, 2_u64] : List<U64>);
assert( li.length == 8 );
assert( li.nth(0_u64).get-or(100_u64) == 1 );
assert( li.nth(1_u64).get-or(100_u64) == 2 );
assert( li.nth(2_u64).get-or(100_u64) == 2 );
assert( li.nth(3_u64).get-or(100_u64) == 3 );
assert( li.nth(4_u64).get-or(100_u64) == 4 );
assert( li.nth(5_u64).get-or(100_u64) == 5 );
assert( li.nth(6_u64).get-or(100_u64) == 3 );
assert( li.nth(7_u64).get-or(100_u64) == 2 );
assert( li.nth(200_u64).get-or(100_u64) == 100_u64 );
