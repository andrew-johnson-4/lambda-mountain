
import LIB/default.lm;

# UmbraShort
let a = to-umbra("Hey");
print(&a);
assert( &a[0_u64] == "H"[0_u64] );
assert( &a[1_u64] == "e"[0_u64] );
assert( &a[2_u64] == "y"[0_u64] );
assert( &a == "Hey" );
a = to-umbra(&a);
assert( &a.length == 3 );
print(&a);
assert( &a == "Hey" );
a = &a + "!";
let hash = deep-hash(&a);
assert( &a.length == 4 );
assert( &a == "Hey!" );
assert( &a != "Hey" );
assert( &a.has-prefix("He") );
assert( not(&a.has-prefix("Ho")) );
