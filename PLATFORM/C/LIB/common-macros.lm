      ( set[] ((macro::lhs-as-rhs base) index rhs) );

macro ( ('macro::define-zero (base-type constructor)) ) (
   non-zero := λ(: t base-type). (: (
      (!=( (.discriminator-case-tag t) (.discriminator-case-tag(: constructor base-type)) ))
   ) U64);
);

