
let .is-arrow(tt: Type): Bool = (
   match tt {
      TAnd { conjugate=conjugate } => (
         let result = false;
         for c in conjugate { result = result or c.is-arrow };
         result
      );
      TGround { tag:c"Arrow" } => true;
      TGround { tag:c"Array", parameters:[_.. array-base..] } => array-base.is-arrow;
      _ => false;
   }
);
