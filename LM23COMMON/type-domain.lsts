
let .domain(tt: Type): Type = (
   match tt {
      TAnd { conjugate=conjugate } => (
         let result = ta;
         for c in conjugate { result = result || c.domain };
         result
      );
      TGround { tag:c"Arrow", parameters:[rng.. dom..] } => dom;
      TGround { tag:c"Array", parameters:[_.. array-base..] } => array-base.domain;
      _ => ta;
   }
);
