
let .without-phi-keep-state(tctx: TypeContext?): TypeContext? = (
   let new-trs = [] : List<TypeContextRow>;
   for list tr in tctx.get-or(mk-tctx()).tctx.reverse {
      new-trs = cons(TypeContextRow(tr.key-or-zero, tr.nt-or-zero.without-phi-keep-state, tr.dt-or-zero.without-phi-keep-state, tr.blame-or-zero), new-trs);
   };
   tctx.with-tctx(new-trs);
);
