
let typeof-var(sloc: AST, tctx: Maybe<TypeContext>, vname: CString, must-not-fresh: U64): (TypeContext?, Type) = (
   (tctx, let found) = typeof-var-raw(sloc, tctx, vname, must-not-fresh);
   if not(non-zero(found)) {
      exit-error(untern("Unknown Referenced Variable: \{vname}"), sloc);
   };
   (tctx, found)
);
