
let .phi-as-initialize(tt: Type): Type = (
   match tt {
      TAnd{conjugate=conjugate} => (
         let new-conjugate = mk-vector(type(Type));
         for c in conjugate {
            new-conjugate = new-conjugate.push(c.phi-as-initialize);
         };
         TAnd(new-conjugate);
      );
      TGround{tag:"Phi::State",parameters:[st..]} => t1(c"Phi::Initialize",st);
      TGround{} => if tt.is-phi-type then t1(c"Phi::Initialize",tt) else tt;
      _ => tt;
   }
);
