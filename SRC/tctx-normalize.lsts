
let .normalize(tctx: TypeContext?): TypeContext? = (
   let new-trs = [] : List<TypeContextRow>;
   for tr in tctx.get-or(mk-tctx()).tctx.reverse {
      new-trs = cons(TypeContextRow(tr.key, normalize(tr.direct-type), normalize(tr.direct-type), tr.blame), new-trs);
   };
   tctx.with-tctx(new-trs);
);
