
let validate-pctx-del(tctx: TypeContext?): Nil = (
   let ps-seen = {} : HashtableEq<CString,U64>;
   for Tuple{pid=first, pt=second} in tctx.get-or(mk-tctx()).pctx {
      if not(ps-seen.has(pid)) {
         #if pt.is-linear
         #then exit-error("", blame);
         ps-seen = ps-seen.bind(pid,true);
      }
   };
);
